#!/bin/bash/

echo " ======================================================"
echo " =                  reinicializando                   ="
echo " ======================================================"
git init

echo " ======================================================"
echo " =                  cambios para agregar              ="
echo " ======================================================"
git status
echo " ======================================================"
echo " =                  cambios para commit               ="
echo " ======================================================"
git add .
git status
echo " ======================================================"
echo " =                  guarda el commit                  ="
echo " ======================================================"
git commit 
echo " ======================================================"
echo " =                  actualizando                      ="
echo " ======================================================"
git pull origin master
echo " ======================================================"
echo " =                  subiendo a GitHub                 ="
echo " ======================================================"
git push origin master

